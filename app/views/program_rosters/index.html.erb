<p id="notice"><%= notice %></p>

<h1>Listing Program Rosters</h1>

<table>
  <thead>
    <tr>
      <th>Program schedule</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @program_rosters.each do |program_roster| %>
      <tr>
        <td><%= program_roster.program_schedule_id %></td>
        <td><%= program_roster.customer_id %></td>
        <td><%= link_to 'Show', program_roster %></td>
        <td><%= link_to 'Edit', edit_program_roster_path(program_roster) %></td>
        <td><%= link_to 'Destroy', program_roster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Program roster', new_program_roster_path %>
