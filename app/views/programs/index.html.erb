<p id="notice"><%= notice %></p>
<h1>Programs/Class Types</h1>

<div class="panel panel-default">
  <div class="panel-body">
    <p id="notice"><%= notice %></p>


    <br><br><%= link_to 'New Program/Class Type', new_program_path, :class=>"btn btn-default" %>

    <div class="table-responsive">
      <table class="table table-striped">
        <thead>
        <tr>
          <th>Program/Class Type</th>

          <th colspan="3"></th>
        </tr>
        </thead>

        <tbody>
        <% @programs.each do |program| %>
            <tr>
              <td><%= program.program_description %></td>
              <td><%= link_to 'More Details', program, :class=> "btn btn-info btn-sm" %></td>
              <td><%= link_to 'Edit', edit_program_path(program), :class=> "btn btn-warning btn-sm" %></td>
              <td><%= link_to 'Delete', program, method: :delete, data: { confirm: 'Are you sure?' }, :class=> "btn btn-danger btn-sm" %></td>
            </tr>
        <% end %>
        </tbody>
      </table>


      <br>


    </div>


  </div>
</div>


