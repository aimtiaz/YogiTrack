<p id="notice"><%= notice %></p>

<h1>Listing Equipment Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Equipment status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @equipment_statuses.each do |equipment_status| %>
      <tr>
        <td><%= equipment_status.equipment_status %></td>
        <td><%= link_to 'Show', equipment_status %></td>
        <td><%= link_to 'Edit', edit_equipment_status_path(equipment_status) %></td>
        <td><%= link_to 'Destroy', equipment_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Equipment status', new_equipment_status_path %>
