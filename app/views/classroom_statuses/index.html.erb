<p id="notice"><%= notice %></p>

<h1>Listing Classroom Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Classroom status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classroom_statuses.each do |classroom_status| %>
      <tr>
        <td><%= classroom_status.classroom_status %></td>
        <td><%= link_to 'Show', classroom_status %></td>
        <td><%= link_to 'Edit', edit_classroom_status_path(classroom_status) %></td>
        <td><%= link_to 'Destroy', classroom_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Classroom status', new_classroom_status_path %>
